{% extends '::base.html.twig' %}

{% block title %}Zeega{% endblock %}



{% block stylesheets %}
    <link rel="shortcut icon" type="image/x-icon" href="http://zeega.org/images/zeega.ico"> 

    <style type="text/css">
    
      
    
        html { 
            background: url({{asset("images/graphpaper.png")}});

        }
    
    </style>

    {% if app.environment == 'dev' %}
        <link rel="stylesheet/less" type="text/css" href="{{ asset('css/less/community/bootstrap.less')}}">
        <script type="text/javascript" src="{{ asset('js//lib/less-1.3.0.min.js')}}"></script>
        <script> /* Provisory for dev environment: */ localStorage.clear(); </script>
    {% else %}
        <link rel="stylesheet" type="text/css" href="{{ asset('css/zeega.community.css')}}">
    {% endif %}

    <link href='http://fonts.googleapis.com/css?family=Podkova:400,700' rel='stylesheet' type='text/css'> <!-- Including Google's Font -->
    <link href='http://fonts.googleapis.com/css?family=Open+Sans+Condensed:700' rel='stylesheet' type='text/css'>
    <script type="text/javascript" language="Javascript" src="{{ asset('js/lib/less-1.3.0.min.js') }}"></script>
    
{% endblock stylesheets %}
{% block header %}

{% endblock header %}
{% block body %}


    <div class="container sign-in">
    
                <div class='sign-in-head'>
                    <a class="brand" href="{{path('ZeegaCommunityBundle_home')}}"><img src="{{ asset('images/zeega-logo-large.png') }}" alt="zeega-logo-large" width="251" height="79"></a>
                    <h2>Make the web you want</h2>
                </div>
                <h2 class='message'>Sorry! We can't find the page you requested!</h2>
                <h2 class='message'>Give us a chance to make it up to you by <a href="mailto:info@zeega.com">emailing</a> us and letting us know what you were looking for.</h2>
                
    
    </div>


{% endblock body %}
{% block footer %}

{% endblock footer %}
