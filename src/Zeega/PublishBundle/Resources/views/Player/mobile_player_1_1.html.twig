{% extends "ZeegaPublishBundle:Player:mobile_player.html.twig" %}

{% block player_stylesheets %}

    <base href="/js/app/player.mobile_1_1/">

    {% if app.environment == 'prod' %}

    <link rel="stylesheet" type="text/css" href="{{ asset('js/app/player.mobile_1_1/dist/release/index.css') }}">
    
    {% else %}

    <link rel="stylesheet" type="text/css" href="{{ asset('js/app/player.mobile_1_1/dist/debug/index.css') }}">

    {% endif %}

{% endblock player_stylesheets %}

{% block player_scripts %}

    {% if app.environment == 'prod' %}

    <script src="{{ asset('js/app/player.mobile_1_1/dist/release/require.js') }}" ></script>

    {% else %}

    <script src="{{ asset('js/app/player.mobile_1_1/dist/debug/require.js') }}" ></script>

    {% endif %}

{% endblock player_scripts %}