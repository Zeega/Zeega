	</div>
	<div id="clear"><br></div>

				
				
				<input type='hidden' id='widget-id' name='widget-id' value='{{widget_id}}'/>
				<input type='hidden' id='playground-id'  name='playground-id' value='{{playground.id}}'/>
	<div style="float:left;">
	<label for="license">Licensing</label>
		<span id="license" >{{license}}</span>
		</div>

	<div  style="float:right; width:180px;">
		
		<ul>
			<li class="AddtoDB">
				<input type="submit" class='add-media-submit' action="" value="Add to Database" id="saveButton" />
			</li>
		
		</ul>	
	</div>

