		<div id="locationDetails">	
			<ul>
				<li>
					<label for="geo-lat">Latitude</label>
						<input name="geo-lat" id="geo-lat" type="text" value="{{geo_lng}}">
				</li>
				<li>
					<label for="geo-lng">Longitude</label>
						<input name="geo-lng" id="geo-lng" type="text" value="{{geo_lat}}">
				</li>
				<li>
					<label for="geo-city">City</label>
						<input name="geo-city" id="geo-city" type="text">
				</li>
				<li>
					<label for="geo-state">State</label>
						<input name="geo-state" id="geo-state" type="text">
				</li>
				<li>
					<label for="geo-neighborhood">Neighborhood</label>
						<input name="geo-neighborhood" id="geo-neighborhood" type="text">
				</li>
			</ul>				
