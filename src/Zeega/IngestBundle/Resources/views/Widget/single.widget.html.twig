

{# src/Zeega/IngestBundle/Resources/views/Widget/fail.widget.html.twig #}

{% extends '::widget.base.html.twig' %}

{% block title %}Zeega Alpha{% endblock %}

{% block body %}


	
	<div  style="text-align:left; width:395px; height: 60px;" >
		<ul class='add-to-database' >
			<li class="AddtoDB">
				<input type="submit" class='add-media-submit' value="Add to Database" id="saveButton" />
				
			</li>
		
		</ul>	
	</div><br />
	


<div id="mediaThumb">
	{% if image is defined %}
	
	<img style="" id="image" src="{{item_url}}" class="scaled" '="">
	{% endif %}
	{% if video is defined %}
	
	<video width="375" controls="true">
  		<source src="{{item_url}}" type="video/mp4; codecs=&quot;avc1.64001E, mp4a.40.2&quot;">
  	</video>
  	{% endif %}
  	
  	{% if audio is defined %}
  	<audio class="extAudio" controls="true" src="{{item_url}}"></audio>
	{% endif %}
	
	
</div>
	
<form id="media_upload_form">

	<div style="width:395px;">
	<br><br>
		<div id="mediaTitleLeft" style='float:left'>
			<label for="title">Title</label>
				<input name="title" id="title"  type="text" value="{{title}}">
		</div>

	<div id="clear"></div>


		<div id="creator" style='float:left;'>
			<label for="creator">Author</label>
				<input name="creator" type="text" style="width:180px;" value="{{creator}}">
		</div>
		
		<div id="alt_creator" style='float:right;'>
			<label for="alt_creator">Author Handle</label>
				<input name="alt_creator"  style="width:180px;"  value="{{alt_creator}}">
		</div>	

	<div id="clear"></div>
	
	

		<div id="content_type"style='float:left;'>
			<label for="content_type">Content Type</label>
				<input name="content_type" style="width:180px;"  value="{{content_type}}">
		</div>
		<div id="date_created_start" style='float:right;'>
			<label for="date_created_start">Date Created</label>
				<input name="date_created_start"  style="width:180px;"  type="text" value="">
		</div>	

	<div id="clear"></div>
	


	<div id="mediaDescriptionWrapper">
		<label for="description">Description</label>
		<textarea type="textarea" name="description" id="description">{{description}}</textarea>
	</div>	
	
	<label for="tags">Tags (comma separated)</label>
		<textarea type="textarea" name="tags" id="tags">{{tags}}</textarea>
	
</div>


	<div id="location">
		
			<label for="mapSearch">Location</label>
			<input style="width:330px" name="map-search" id="map-search-input" type="text">
				
